{
  "name": "@alehuo/clubhouse-shared",
  "version": "1.2.6",
  "description": "Shared libraries for Clubhouse front-end and back-end.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": "https://github.com/alehuo/clubhouse-shared",
  "author": "alehuo",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.12",
    "chai": "^4.2.0",
    "husky": "^1.2.1",
    "mocha": "^5.2.0",
    "moment": "^2.23.0",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "typescript": "3.2.2"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist/*.d.ts dist/*.js dist/*.d.ts.map dist/*.js.map",
    "test": "nyc mocha test/**/*.test.ts --exit --timeout 10000"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/**/*.ts",
      "dist/**/*.js",
      "coverage/**/*.js",
      "src/index.ts",
      "src/Permissions.ts"
    ],
    "reporter": [
      "html",
      "text",
      "lcov"
    ],
    "perFile": true,
    "all": true,
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn clean && yarn build && yarn test"
    }
  }
}
